#include <stdio.h>

int main() {
	
	float num1;
	float num2;
	float num3;
	
	printf("Digite uma nota\n");
	scanf("%f", &num1);
	printf("Digite outra nota\n");
	scanf("%f", &num2);
	printf("Digite a ultima nota\n");
	scanf("%f", &num3);
	
	float resultado = (num1 + num2 + num3) /3;
	
	printf("A nota final e: %.2f", resultado);
}
