#include <stdio.h>

int main () {
	int nota1,nota2,nota3;
	
	printf("Digite a primeira nota\n");
	scanf("%i", &nota1);
	printf("Digite a segunda nota\n");
	scanf("%i", &nota2);
	printf("Digite a terceira nota\n");
	scanf("%i", &nota3);
	
	int resultado = (nota1+nota2+nota3)/3;
	
	if(resultado >= 6) {
		printf("Aprovado");
	}
	
	else{
		printf("Reprovado");
		
	}
}
